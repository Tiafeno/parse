{
    "name": "cpp/parse",
    "description": "An Eloquent-like interface for Parse",
    "keywords": ["laravel", "parse", "sdk", "parse-sdk", "eloquent"],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "laravel/framework": "^7.0|^8.0" ,
        "parse/php-sdk": "^1.2.0"
    },
    "autoload": {
        "psr-4": {
            "Cpp\\Parse\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cpp\\Parse\\ParseServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    }
}
